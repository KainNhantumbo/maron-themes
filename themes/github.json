    },
    {
      "name": "Export",
      "scope": [
        "keyword.control.export",
        "keyword.import",
        "meta.import",
        "entity.name.function.operator"
      ],
      "settings": {
        "foreground": "#7ee787"
      }
    }
  ]
}
