    "dropdown.listBackground": "#1F1F1F",

    "editor.background": "#1F1F1F",
    "editor.foreground": "#f8f8f2",
    "selection.background": "#3e3d3270",
    "editor.selectionHighlightBackground": "#1a2c303b",
    "editor.selectionBackground": "#3e3d3270",
    "editor.wordHighlightBackground": "#1a2c303b",
    "editor.wordHighlightStrongBackground": "#6a6a9680",
    "editor.lineHighlightBackground": "#08080875",
    "editorLineNumber.activeForeground": "#c2c2bf",
    "editorCursor.foreground": "#f8f8f0",
    "editorWhitespace.foreground": "#464741",
    "editorGroupHeader.tabsBackground": "#1F1F1F",
    "editorGroup.dropBackground": "#41433980",

    "editorIndentGuide.activeBackground1": "#464741",
    "editorIndentGuide.activeBackground2": "#464741",
    "editorIndentGuide.activeBackground3": "#464741",
    "editorIndentGuide.activeBackground4": "#464741",
    "editorIndentGuide.activeBackground5": "#464741",
    "editorIndentGuide.activeBackground6": "#464741",
    "editorIndentGuide.background1": "#46474180",
    "editorIndentGuide.background2": "#46474180",
    "editorIndentGuide.background3": "#46474180",
    "editorIndentGuide.background4": "#46474180",
    "editorIndentGuide.background5": "#46474180",
    "editorIndentGuide.background6": "#46474180",

    "minimap.selectionHighlight": "#1a2c303b",

    "tab.inactiveBackground": "#1F1F1F",
    "tab.border": "#1F1F1F",
    "tab.activeBorderTop": "#AC6218",
    "tab.inactiveForeground": "#ccccc7",
    "tab.lastPinnedBorder": "#414339",

    "widget.shadow": "#00000068",
    "widget.border": "#AC621850",
    "progressBar.background": "#1F1F1F",
    "badge.background": "#1F1F1F",
    "badge.foreground": "#f8f8f2",
    "editorLineNumber.foreground": "#90908a",
    "panelTitle.activeForeground": "#f8f8f2",
    "panelTitle.activeBorder": "#1F1F1F",
    "panelTitle.inactiveForeground": "#1F1F1F",
    "panel.border": "#41433960",
    "settings.focusedRowBackground": "#4143395A",
    "titleBar.activeBackground": "#1F1F1F",

    "statusBar.background": "#1F1F1F",
    "statusBar.noFolderBackground": "#414339",
    "statusBar.debuggingBackground": "#1F1F1F",
    "statusBarItem.remoteBackground": "#AC6218",

    "activityBar.background": "#1F1F1F",
    "activityBar.foreground": "#AC6218",
    "activityBarBadge.background": "#AC6218",

    "sideBar.background": "#1F1F1F",
    "sideBarSectionHeader.background": "#1F1F1F",

    "menu.background": "#1F1F1F",
    "menu.foreground": "#cccccc",
    "pickerGroup.foreground": "#1F1F1F",
    "input.background": "#1F1F1F",
    "input.border": "#3e3d3270",
    "focusBorder": "#3e3d3270",
    "editorWidget.background": "#1F1F1F",
    "debugToolBar.background": "#1F1F1F",
    "diffEditor.insertedTextBackground": "#4b661680",
    "diffEditor.removedTextBackground": "#90274A70",
    "inputValidation.errorBackground": "#90274A",
    "inputValidation.errorBorder": "#f92640",
    "inputValidation.warningBackground": "#848528",
    "inputValidation.warningBorder": "#e2e22e",
    "inputValidation.infoBackground": "#546190",
    "inputValidation.infoBorder": "#819aff",
    "editorHoverWidget.background": "#1F1F1F",
    "editorHoverWidget.border": "#AC6218",
    "editorSuggestWidget.background": "#1F1F1F",
    "editorSuggestWidget.border": "#AC6218",
    "editorGroup.border": "#1F1F1F",
    "inputOption.activeBorder": "#3e3d3270",

    "peekView.border": "#1F1F1F",
    "peekViewEditor.background": "#1F1F1F",
    "peekViewResult.background": "#1F1F1F",
    "peekViewTitle.background": "#1F1F1F",
    "peekViewResult.selectionBackground": "#3e3d3270",
    "peekViewResult.matchHighlightBackground": "#1F1F1F",
    "peekViewEditor.matchHighlightBackground": "#1F1F1F",

    "terminal.ansiBlack": "#333333",
    "terminal.ansiRed": "#C4265E",
    "terminal.ansiGreen": "#86B42B",
    "terminal.ansiYellow": "#B3B42B",
    "terminal.ansiBlue": "#6A7EC8",
    "terminal.ansiMagenta": "#8C6BC8",
    "terminal.ansiCyan": "#56ADBC",
    "terminal.ansiWhite": "#e3e3dd",
    "terminal.ansiBrightBlack": "#666666",
    "terminal.ansiBrightRed": "#f92640",
    "terminal.ansiBrightGreen": "#A6E22E",
    "terminal.ansiBrightYellow": "#e2e22e",
    "terminal.ansiBrightBlue": "#819aff",
    "terminal.ansiBrightMagenta": "#AE81FF",
    "terminal.ansiBrightCyan": "#66D9EF",
    "terminal.ansiBrightWhite": "#f8f8f2",
    "terminal.border": "#41433960",
    "terminal.tab.activeBorder": "#e2e22e"
  },
  "tokenColors": [
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation.definition.tag",
        "punctuation",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#86897A"
      }
    },
    {
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#88846f"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "Template Definition",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#F92640"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#F92640"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#F92640"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "",
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "",
        "foreground": "#FD971F"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#F92640"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Library class/type",
      "scope": "support.type, support.class",
      "settings": {
        "fontStyle": "",
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "",
        "foreground": "#F44747"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "name": "JSON String",
      "scope": "meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#CFCFC2"
      }
    },
    {
      "name": "diff.header",
      "scope": "meta.diff, meta.diff.header",
      "settings": {
        "foreground": "#1F1F1F"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#F92640"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#AE81FFA0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#F92640"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "Markup Styling",
      "scope": "markup.bold, markup.",
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#FD971F"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#A6E22E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "fontStyle": "",
        "foreground": "#FD971F"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link Title/Description",
      "scope": "string.other.link.title.markdown,string.other.link.description.markdown",
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Markdown Emphasis",
      "scope": "markup..markdown",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Markdown Punctuation Definition Link",
      "scope": "markup.list.unnumbered.markdown, markup.list.numbered.markdown",
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "name": "Markdown List Punctuation",
      "scope": ["punctuation.definition.list.begin.markdown"],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#FD971F"
      }
    }
  ],
  "semanticHighlighting": true
}
