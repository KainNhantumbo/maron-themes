{
  "name": "Maron Monokai",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "author": "Kain Nhantumbo",
  "maintainers": ["Kain Nhantumbo <nahantumbok@gmail.com>"],
  "colors": {
