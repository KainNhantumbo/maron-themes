      "settings": {
        "foreground": "#E6C384"
      }
    },
    {
      "name": "HTML Tag",
      "scope": ["entity.name.tag.html"],
      "settings": {
        "foreground": "#957FB8"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#7E9CD8"
      }
