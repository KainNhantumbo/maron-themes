    {
      "name": "Export",
      "scope": [
        "keyword.control.export",
        "keyword.control.default",
        "keyword.import",
        "meta.import",
        "entity.name.function.operator"
      ],
      "settings": {
        "foreground": "#c5e4fd"
      }
    }
  ]
