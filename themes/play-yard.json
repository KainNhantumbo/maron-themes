  "tokenColors": [
    {
      "scope": ["keyword.operator.new", "keyword.operator.logical"],
      "settings": {
        "foreground": "#BA8EF7"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#707A84"
      }
    },
    {
      "scope": ["comment.line.shebang"],
      "settings": {
        "foreground": "#FF8AD1"
      }
    },
    {
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#A8B2BA"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF6A80"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#BA8EF7"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#939DA5"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#BA8EF7"
      }
    },
    {
      "scope": ["constant.language", "variable.other.property"],
      "settings": {
        "foreground": "#FFA763"
      }
    },
    {
      "name": "Variable / object  property",
      "scope": [
        "variable.other.property",
        "variable.other.property.static",
        "variable.other.event"
      ],
      "settings": {
        "foreground": "#FFA763"
      }
    },
    {
      "name": "text",
      "scope": "text",
      "settings": {
        "foreground": "#e2e2e3"
      }
    },
    {
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.definition.typeparameters",
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#939DA5"
      }
    },
    {
      "scope": ["support.function.builtin"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#FF6A80"
      }
    },
    {
      "scope": ["support.class.component"],
      "settings": {
        "foreground": "#BA8EF7"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#FFEA6B"
      }
    },
    {
      "scope": ["punctuation.definition.arguments", "meta.function-call"],
      "settings": {
        "foreground": "#e2e2e3"
      }
    },
    {
      "scope": ["punctuation.accessor.js", "punctuation.parenthesis"],
      "settings": {
        "foreground": "#e2e2e3"
      }
    },
    {
      "scope": ["variable.other"],
      "settings": {
        "foreground": "#e2e2e3"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "scope": [
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#e2e2e3"
      }
    },
    {
      "scope": ["support.variable.property"],
      "settings": {
        "foreground": "#FFA763"
      }
