{
  "$schema": "vscode://schemas/color-theme",
  "colors": {
    "activityBar.activeBorder": "#586E75",
    "activityBar.background": "#0A1016",
    "activityBar.border": "#586e7530",
    "activityBar.dropBorder": "#586e7530",
    "activityBar.foreground": "#586E75",
    "activityBarBadge.background": "#586E75",
    "activityBarTop.activeBorder": "#586e7530",
    "activityBarTop.dropBorder": "#586e7530",
    "badge.background": "#93A1A1",
    "breadcrumb.activeSelectionForeground": "#93A1A1",
    "breadcrumb.focusForeground": "#93A1A1",
    "breadcrumb.foreground": "#586E75",
    "breadcrumbPicker.background": "#0A1016",
    "commandCenter.background": "#0A1016",
    "dropdown.background": "#0A1016",
    "editor.lineHighlightBackground": "#0A1016",
    "editorCursor.foreground": "#93A1A1",
    "editorGroup.border": "#586e7530",
    "editorGroup.dropBackground": "#586E7520",
    "editorGroup.dropIntoPromptBorder": "#586e7530",
    "editorGroupHeader.border": "#586e7530",
    "editorGroupHeader.tabsBackground": "#0A1016",
    "editorGroupHeader.tabsBorder": "#586e7530",
    "editorGutter.background": "#0A1016",
    "editorHoverWidget.background": "#0A1016",
    "editorLineNumber.activeForeground": "#93A1A1",
    "editorPane.background": "#0A1016",
    "editorWidget.background": "#0A1016",
    "editorWidget.foreground": "#586E75",
    "focusBorder": "#93A1A140",
    "icon.foreground": "#586E75",
    "input.background": "#0A1016",
    "list.activeSelectionBackground": "#586E7545",
    "list.dropBackground": "#586E7520",
    "list.hoverBackground": "#586E7525",
    "list.inactiveSelectionBackground": "#586E7525",
    "list.inactiveSelectionForeground": "#93A1A1",
    "list.inactiveSelectionIconForeground": "#93A1A1",
    "menu.background": "#0A1016",
    "menu.foreground": "#586E75",
    "menu.selectionBackground": "#586E7520",
    "notificationCenterHeader.background": "#0A1016",
    "notifications.background": "#0A1016",
    "panel.background": "#0A1016",
    "panel.border": "#586e7530",
    "panel.dropBorder": "#586e7530",
    "panelTitle.activeBorder": "#93A1A1",
    "panelTitle.activeForeground": "#93A1A1",
    "panelTitle.inactiveForeground": "#586E75",
    "panelInput.border": "#586e7530",
    "panelSection.border": "#586e7530",
    "panelSection.dropBackground": "#586E7520",
    "panelSectionHeader.background": "#0A1016",
    "peekViewResult.background": "#0A1016",
    "quickInput.background": "#0A1016",
    "scrollbar.shadow": "#000000",
    "settings.checkboxBackground": "#0A1016",
    "settings.dropdownBackground": "#0A1016",
    "settings.headerForeground": "#586E75",
    "settings.modifiedItemIndicator": "#586E7590",
    "settings.numberInputBackground": "#0A1016",
    "settings.textInputBackground": "#0A1016",
    "sideBar.background": "#0A1016",
    "sideBar.border": "#586e7530",
    "sideBar.dropBackground": "#586E7520",
    "sideBar.foreground": "#586E75",
    "sideBarSectionHeader.border": "#586e7530",
    "sideBarTitle.foreground": "#93A1A1",
    "statusBar.background": "#0A1016",
    "statusBar.border": "#586e7530",
    "statusBar.foreground": "#586E75",
    "statusBarItem.compactHoverBackground": "#0A1016",
    "statusBarItem.hoverBackground": "#0A1016AA",
    "statusBarItem.hoverForeground": "#93A1A1",
    "statusBarItem.remoteBackground": "#0A1016",
    "tab.activeBorder": "#586e7500",
    "tab.activeBorderTop": "#586E75",
    "tab.activeForeground": "#93A1A1",
    "tab.border": "#586e7530",
    "tab.inactiveBackground": "#0A1016",
    "tab.inactiveForeground": "#586E75",
    "terminal.background": "#0A1016",
    "terminal.border": "#586e7530",
    "terminal.dropBackground": "#586E7520",
    "terminal.tab.activeBorder": "#93A1A1",
    "terminalCursor.foreground": "#93A1A1",
    "titleBar.activeForeground": "#586E75",
    "titleBar.border": "#586e7530",
    "titleBar.inactiveForeground": "#586E75",
    "widget.border": "#586e7530"
  },
  "include": "./solarized-classic.json",
  "name": "Maron Solarized Planets Operator (Lite Edition)",
  "tokenColors": [
    {
      "name": "Export, Default",
      "scope": [
        "keyword.control.export",
        "keyword.control.default",
        "keyword.import",
        "entity.name.function.operator"
      ],
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Punctuation Terminators",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end"
      ],
      "settings": {
        "foreground": "#2AA198"
      }
    },
    {
      "name": "Arrow Functions",
      "scope": [
        "storage.type.cs",
        "storage.type.function.arrow",
        "storage.type.function.name"
      ],
      "settings": {
        "foreground": "#586E75"
      }
    },
    {
      "scope": [
        "constant.character.entity",
        "constant.character.escape",
        "constant.character.format.placeholder.other",
        "entity.name.function.preprocessor",
        "entity.name.section.group-title",
        "keyword.control.directive",
        "meta.attribute",
        "meta.preprocessor.conditional",
        "meta.preprocessor.cs",
        "meta.preprocessor.pragma",
        "punctuation.definition.directive",
        "punctuation.definition.entity",
        "punctuation.definition.variable",
        "punctuation.definition.block",
        "punctuation.separator.continuation",
        "source.c constant.other.placeholder",
        "source.rust support.function.std",
        "storage.modifier.lifetime",
        "string.regexp.quoted.single",
        "support.function.core.rust",
        "variable.other.bracket",
        "variable.other.normal",
        "variable.other.positional",
        "variable.other.special"
      ],
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "punctuation.accessor",
      "scope": ["punctuation.accessor", "punctuation"],
      "settings": {
        "foreground": "#586E75"
      }
    },
    {
      "name": "Braces, delimiters",
      "scope": [
        "meta.brace.round",
        "meta.brace.square",
        "meta.brace.curly",
        "constant.other.color",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution",
        "meta.delimiter.comma",
        "meta.function-call.without-arguments.js",
        "meta.function-call.method.without-arguments.js"
      ],
      "settings": {
        "foreground": "#586E75"
      }
    },
    {
      "name": "Punctuation tags",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag source",
        "punctuation.definition.group.begin.ruby",
        "punctuation.definition.group.end.ruby",
        "punctuation.definition.group.begin.css",
        "punctuation.definition.group.end.css",
        "punctuation.definition.string.end.html source.css"
      ],
      "settings": {
        "foreground": "#586E75"
      }
    },
    {
      "name": "Custom tags JSX",
      "scope": [
        "entity.name.tag.other.html",
        "entity.name.tag support.class.component",
        "support.class.component.html"
      ],
      "settings": {
        "foreground": "#859900"
      }
    },

    {
      "name": "Meta import type keyword",
      "scope": ["keyword.control.type"],
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Import meta",
      "scope": ["meta.import"],
      "settings": {
        "foreground": "#93A1A1"
      }
    },
    {
      "name": "JSDoc storage type",
      "scope": "comment storage.type",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function arguments",
      "scope": ["variable.parameter", "parameters variable.function"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords and Expressions",
      "scope": [
        "keyword.control.export",
        "keyword.control.import",
        "keyword.control.type",
        "keyword.control.as",
        "keyword.control.conditional",
        "keyword.control.flow",
        "keyword.control.or",
        "keyword.control.and",
        "keyword.control.loop",
        "keyword.operator.new",
        "keyword.operator.expression.of",
        "keyword.operator.expression.instanceof",
        "keyword.operator.expression.typeof",
        "keyword.operator.expression.keyof",
        "keyword.operator.expression.delete",
        "keyword.control.trycatch",
        "keyword.control.from",
        "variable.language.this",
        "storage.modifier.async",
        "keyword.control.default",
        "keyword.import",
        "entity.name.function.operator"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name", "storage.modifier"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["comment", "invalid", "invalid.deprecated", "invalid.illegal"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end"
      ],
      "settings": {
        "foreground": "#268bd2"
      }
    }
  ]
}
