  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#B2B2B2"
      }
    },
    {
      "scope": ["keyword", "storage.type.class"],
      "settings": {
        "foreground": "#C5A5C5"
      }
    },
    {
      "scope": "function",
      "settings": {
        "foreground": "#79B6F2"
      }
    },
    {
      "scope": ["variable", "meta.jsx.children"],
      "settings": {
        "foreground": "#D7DEEA"
      }
    },
    {
      "scope": [
        "type",
        "support.type",
        "support.type.property-name.json.comments"
      ],
      "settings": {
        "foreground": "#FAC863"
      }
    },
    {
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#8BE9EE"
      }
    },
    {
      "scope": "markup.heading.1 punctuation.definition.heading",
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "scope": "constant.character.entity",
      "settings": {
        "foreground": "#D67C9B"
      }
    },
    {
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#D67C9B"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "support.class.builtin",
        "keyword.other.name-of-parameter"
      ],
      "settings": {
        "foreground": "#79B6F2"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.underline.link",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#FC929E"
      }
    },
    {
      "scope": [
        "support.class",
        "support.class.component",
        "constant.numeric",
        "support.variable.property.dom"
      ],
      "settings": {
        "foreground": "#FC929E"
      }
    },
    {
      "scope": ["entity.other.inherited-class", "entity.name.type"],
      "settings": {
        "foreground": "#FAC863"
      }
    },
    {
      "scope": ["storage.modifier", "storage.type"],
      "settings": {
        "foreground": "#C5A5C5"
      }
    },
    {
      "scope": ["constructor"],
      "settings": {
        "foreground": "#79B6F2"
      }
    },
    {
      "scope": [
        "variable.language",
        "entity.other.attribute-name",
        "constant.language.undefined",
        "constant.language.null"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#C5A5C5"
      }
    },
    {
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#FF8B50"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "variable.other.constant",
        "variable.other.readwrite",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#D7DEEA"
      }
    },
    {
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
        "meta.brace.round",
        "meta.array.literal"
      ],
      "settings": {
        "foreground": "#88C6BE"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#fac863DD"
      }
    },
    {
      "scope": "source.python",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": "constant.language.python",
      "settings": {
        "foreground": "#F99157"
      }
    },
    {
      "scope": "source.lua",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#B2B2B2"
      }
    },
    {
      "scope": "punctuation.terminator.statement",
      "settings": {
        "foreground": "#88C6BE"
      }
    },
    {
      "scope": "punctuation.definition.comment",
      "settings": {
        "foreground": "#B2B2B2"
      }
    },
    {
      "scope": "string.quoted",
      "settings": {
        "foreground": "#8DC891"
      }
    },
    {
      "scope": "constant.other.object.key",
      "settings": {
        "foreground": "#D8DEE9"
      }
    },
    {
      "scope": "meta.embedded.assembly",
      "settings": {
        "foreground": "#8DC891"
      }
    },
    {
      "scope": "keyword - keyword.operator",
      "settings": {
        "foreground": "#C5A5C5"
      }
    },
    {
      "scope": "keyword.operator.new",
      "settings": {
        "foreground": "#C5A5C5"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#C5A5C5"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#C5A5C5"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#C5A5C5"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#5A9BCF"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#FAC863"
      }
    },
    {
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#88C6BE"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#D8DEE9"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#88C6BE"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#79B6F2"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#79B6F2"
      }
    },
    {
      "scope": "entity.name.variable",
      "settings": {
        "foreground": "#FC929E"
      }
    },
    {
      "scope": [
        "meta.class",
        "entity.name.class",
        "entity.name.type.class",
        "entity.other.inherited-class",
        "meta.field.declaration"
      ],
      "settings": {
        "foreground": "#F9C76B"
      }
    },
    {
      "scope": [
        "variable.other.readwrite",
        "meta.function.parameters",
        "meta.function.method"
      ],
      "settings": {
        "foreground": "#D8DEE9"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation.definition.tag",
        "punctuation",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution",
        "meta.brace.round",
        "meta.brace.square",
        "meta.brace.curly",
        "constant.other.color",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution",
        "meta.delimiter.comma.js",
        "meta.function-call.without-arguments.js",
        "meta.function-call.method.without-arguments.js",
        "punctuation.definition.tag",
        "punctuation.definition.tag source",
        "punctuation.definition.group.begin.ruby",
        "punctuation.definition.group.end.ruby",
        "punctuation.definition.group.begin.css",
        "punctuation.definition.group.end.css",
        "punctuation.definition.string.end.html source.css"
      ],
      "settings": {
        "foreground": "#BFD5DE60"
      }
    },
    {
      "name": "Export, Default",
      "scope": [
        "keyword.control.export",
        "keyword.control.default",
        "keyword.import",
        "entity.name.function.operator"
      ],
      "settings": {
        "foreground": "#fac863"
      }
    }
  ],
  "type": "dark"
}
